        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".track.TrackAcquisitionUploadActivity"
            android:description="@string/desc_activity_track_upload"
            android:enabled="true"
            android:label="@string/title_activity_track_upload"
            android:parentActivityName=".main.TrackActivity"></activity>
        <activity
            android:name=".overlay.LogisticsStateActivity"
            android:description="@string/desc_activity_logistics"
